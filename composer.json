{
    "name": "bert-w/runescape-hiscores-api",
    "description": "Retrieve Hiscore results easily from the OSRS RuneScape Hiscores page.",
    "keywords": ["runescape", "hiscores", "rs07", "osrs"],
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "symfony/dom-crawler": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "BertW\\RunescapeHiscoresApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BertW\\RunescapeHiscoresApi\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.1",
        "mockery/mockery": "^1.4"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --debug --coverage-html coverage",
        "pcf": "vendor/bin/php-cs-fixer fix --verbose"
    }
}
